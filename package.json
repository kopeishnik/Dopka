{
  "name": "mp3-strip",
  "version": "0.0.1",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "lint": "eslint . --no-error-on-unmatched-pattern --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix --ignore-path .gitignore",
    "format": "prettier . --ignore-path .gitignore --plugin-search-dir=. .",
    "format:fix": "prettier --write . --ignore-path .gitignore",
    "lint:style": "npx stylelint \"src*/*.+(scss|css|svelte)\" --ignore-path .gitignore ",
    "lint:style:fix": "stylelint \"src*/*.+(scss|css|svelte)\" --fix --ignore-path .gitignore",
    "start" : "node server.js",
    "devstart" : "nodemon server.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "svelte": "^3.44.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
